# AutoEQ Environment Configuration
# Copy to .env and fill in values

# Groq API Key (required for AI clustering)
GROQ_API_KEY=your_groq_api_key_here

# SoundCloud username to scrape (optional, defaults to amr-farouk-10)
SOUNDCLOUD_USERNAME=amr-farouk-10

# Max tracks to fetch (optional, defaults to 10000)
MAX_TRACKS=10000
